	<span class="pl-c">&lt;!-- 实例化编辑器 --&gt;</span>
	<span class="pl-kos">&lt;</span><span class="pl-ent">script</span> <span class="pl-c1">type</span>="<span class="pl-s">text/javascript</span>"<span class="pl-kos">&gt;</span>
	    <span class="pl-k">var</span> <span class="pl-s1">ue</span> <span class="pl-c1">=</span> <span class="pl-c1">UE</span><span class="pl-kos">.</span><span class="pl-en">getEditor</span><span class="pl-kos">(</span><span class="pl-s">'container'</span><span class="pl-kos">)</span><span class="pl-kos">;</span>
	<span class="pl-kos">&lt;/</span><span class="pl-ent">script</span><span class="pl-kos">&gt;</span>
